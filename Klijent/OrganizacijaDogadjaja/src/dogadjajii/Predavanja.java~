import java.util.*;
import java.io.*;

public class Predavanja extends Dogadjaj
{
  protected String tema;
  protected String predavac;
  protected String sadrzaj;
  
  public Predavanja()
  {
    super();
  }
  
  
  public Predavanja(String te,String pre,String sa,String n,Date dp,Date dk,String vp,String vk,String o,Organizator org,ArrayList<Ucesnik>uc,File f)
  {
    super(n,dp,dk,vp,vk,o,org,uc,f);
    tema=te;
    predavac=pre;
    sadrzaj=sa;
  }
  public String getTema()
  {
    return tema;
  }
  public String getPredavac()
  {
    return predavac;
  }
  public String getSadrzaj()
  {
    return sadrzaj;
  }
  
